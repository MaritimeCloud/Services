namespace dma.messaging;
enum MaritimeTextingNotificationSeverity {
    MESSAGE = 1;
    WARNING = 2;
    ALERT  = 3;
}
message MaritimeText {
    1: MaritimeTextingNotificationSeverity severity;
    2: text msg;
}
endpoint MaritimeTextingService {
	void sendMessage(
        1: MaritimeText msg
    );
}
